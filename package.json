{
  "name": "toilet-finder",
  "version": "1.0.0",
  "description": "IBS Health Tracker - Find toilets near you",
  "main": "server/index.ts",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx server/index.ts",
    "dev:client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "build:server": "tsc server/index.ts --outDir dist --target es2020 --module commonjs",
    "start": "NODE_ENV=production tsx server/index.ts",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "install:all": "npm install && cd client && npm install",
    "migrate": "node setup_and_migrate_all.mjs",
    "check-quota": "node check_firebase_quota.mjs",
    "test": "cd client && npm test",
    "lint": "cd client && npm run lint",
    "deploy:build": "npm run build && npm run build:server"
  },
  "keywords": [
    "toilet",
    "finder",
    "ibs",
    "health",
    "tracker",
    "bulgaria"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@neondatabase/serverless": "^0.7.2",
    "@supabase/supabase-js": "^2.53.0",
    "drizzle-orm": "^0.29.1",
    "drizzle-zod": "^0.5.1",
    "express": "^4.18.2",
    "firebase-admin": "^12.0.0",
    "nanoid": "^5.0.4",
    "node-fetch": "^3.3.2",
    "tsx": "^4.6.2",
    "vite": "^5.0.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "concurrently": "^8.2.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
